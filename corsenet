# Create the Conv2D layer
filter_img_imag_layer = Conv2D(filters_sin.shape[3], 
                               (filters_sin.shape[0], filters_sin.shape[1]), 
                               padding='same', 
                               name='enh_img_imag_1')

# Set the weights for the layer
filter_img_imag_layer.build(input_shape=img_input.shape)
filter_img_imag_layer.set_weights([filters_sin, np.zeros([filters_sin.shape[3]])])

# Apply the layer to the input
filter_img_imag = filter_img_imag_layer(img_input)
