import tensorflow as tf
from tensorflow.keras import backend as K
import argparse

# Disable eager execution (if you need it for compatibility)
tf.compat.v1.disable_eager_execution()

# There is no need for GPU configuration since you don't have a GPU
# Just set the session for Keras (this is usually not necessary in TensorFlow 2.x, but if needed)
sess = tf.compat.v1.Session()
tf.compat.v1.keras.backend.set_session(sess)

