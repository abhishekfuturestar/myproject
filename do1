import fingerprint_enhancer								# Load the library
import cv2

img = cv2.imread('C:/Users/2179048/Downloads/Fingerprint-Enhancement-Python-develop/Fingerprint-Enhancement-Python-develop/images/1.jpg', 0)						# read input image
out = fingerprint_enhancer.enhance_fingerprint(img)		
cv2.imshow('enhanced_image', out);		
cv2.waitKey(0)	
